{
  "manifest_version": 3,
  "name": "NoIndex Detector",
  "version": "1.0",
  "description": "Detecta 'noIndex' e 'noindex' em páginas específicas.",
  "permissions": ["activeTab"],
  "icons": {
      "48": "icon.png"
  },
  "content_scripts": [
      {
          "matches": ["https://bitbucket.org/*"],
          "js": ["content.js"]
      }
  ]
}
